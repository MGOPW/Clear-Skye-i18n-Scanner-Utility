        <api_name>x_cls_i18n_scan.runStringScan</api_name>
        <caller_access/>
        <client_callable>false</client_callable>
        <description>Scan for Strings that may need to be converted to gs.getMessage statements and log them to the Scanned Node table</description>
        <name>runStringScan</name>
        <script><![CDATA[var runStringScan = function(scanId) {
	var config = getScanTableConfigs(scanId);

	try {
		runScriptScanner(config);
	}
	catch(err) {
		gs.debug(JSON.stringify(err));
	}
};]]></script>
