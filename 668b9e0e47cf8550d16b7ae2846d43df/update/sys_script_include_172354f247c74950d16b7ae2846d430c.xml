<?xml version="1.0" encoding="UTF-8"?><record_update table="sys_script_include">
    <sys_script_include action="INSERT_OR_UPDATE">
        <access>package_private</access>
        <active>true</active>
        <api_name>x_cls_i18n_scan.isConcatenated</api_name>
        <caller_access/>
        <client_callable>false</client_callable>
        <description>Determines if a string within a script is potentially concatenated</description>
        <name>isConcatenated</name>
        <script><![CDATA[var isConcatenated = function(script, stringStart, stringEnd) {
	var stringContext = script.substring(stringStart-3, stringEnd + 3);
	
	return stringContext.indexOf('+') != -1;
};]]></script>
