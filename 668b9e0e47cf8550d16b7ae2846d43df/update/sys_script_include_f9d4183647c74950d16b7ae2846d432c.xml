    <sys_script_include action="INSERT_OR_UPDATE">
        <access>package_private</access>
        <active>true</active>
        <api_name>x_cls_i18n_scan.usesGetMessage</api_name>
        <caller_access/>
        <client_callable>false</client_callable>
        <description>Determines if a string within a script is potentially already wrapped in a gs.getMessage call</description>
        <name>usesGetMessage</name>
        <script><![CDATA[var usesGetMessage = function(script, stringStart, stringEnd) {
	var stringContext = script.substring(stringStart - 20, stringEnd);
	return stringContext.indexOf('gs.getMessage(') != -1;
};]]></script>
